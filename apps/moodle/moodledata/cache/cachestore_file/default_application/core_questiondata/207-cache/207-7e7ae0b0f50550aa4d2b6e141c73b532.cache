O:8:"stdClass":24:{s:2:"id";s:3:"207";s:8:"category";s:2:"83";s:6:"parent";s:1:"0";s:4:"name";s:23:"【待完善】火星20";s:12:"questiontext";s:1565:"<p dir="ltr" style="text-align: left;"></p><h3 style="text-align: center;"><strong>来自“好奇者”的礼物&nbsp;</strong></h3><p style="text-align: center;">玛丽亚·格林</p><p dir="ltr" style="text-align: left;">几十年来，科学家们一直好奇火星表面是否存在水。现在，“好奇者”返回的照片显示火星曾经有过河流。其中，有的照片展现了可能是干枯的河床，有的照片展示了可能由于河流冲击而造成的巨大峡谷和山谷。 科学家们现在“确信火星在几十亿年前拥有海洋”，负责“好奇者”探测任务的查尔斯·埃拉奇说。 但是，如果火星上曾存在过一个古老的海洋，这就引发了另一个问题，埃拉奇说： “生物是否会在火星上进化？”如果能，“它们现在又在哪里？”</p><p dir="ltr" style="text-align: center;"><img src="https://moodlenlm.xyz/moodle/pluginfile.php/1/question/questiontext/467/18/206/mars-rocks.jpg" alt="火星上的石头" width="150" height="238" class="img-fluid atto_image_button_text-bottom"><img src="https://moodlenlm.xyz/moodle/pluginfile.php/1/question/questiontext/467/18/206/earth-rocks.jpg" alt="地球上的石头" width="150" height="238" class="img-fluid atto_image_button_text-bottom"></p><p dir="ltr" style="text-align: center;">火星上的石头（左）和地球上的石头（右）</p><p dir="ltr" style="text-align: left;"><strong>《来自“好奇者”的礼物》的作者赞成对火星的探测吗？你从哪些地方看出来的？</strong></p><p></p>";s:18:"questiontextformat";s:1:"1";s:15:"generalfeedback";s:0:"";s:21:"generalfeedbackformat";s:1:"1";s:11:"defaultmark";d:1;s:7:"penalty";d:0.3333333;s:5:"qtype";s:11:"multichoice";s:6:"length";s:1:"1";s:5:"stamp";s:33:"moodlenlm.xyz+221111170948+cK229l";s:7:"version";s:33:"moodlenlm.xyz+221125050924+gznGKp";s:6:"hidden";s:1:"1";s:11:"timecreated";s:10:"1668186588";s:12:"timemodified";s:10:"1669352964";s:9:"createdby";s:1:"2";s:10:"modifiedby";s:1:"2";s:8:"idnumber";N;s:9:"contextid";s:1:"3";s:7:"options";O:8:"stdClass":15:{s:2:"id";s:3:"124";s:10:"questionid";s:3:"207";s:6:"layout";s:1:"0";s:6:"single";s:1:"1";s:14:"shuffleanswers";s:1:"1";s:15:"correctfeedback";s:27:"您的答案是正确的。";s:21:"correctfeedbackformat";s:1:"1";s:24:"partiallycorrectfeedback";s:27:"您的答案部分正确。";s:30:"partiallycorrectfeedbackformat";s:1:"1";s:17:"incorrectfeedback";s:21:"您的答案不正确";s:23:"incorrectfeedbackformat";s:1:"1";s:15:"answernumbering";s:3:"abc";s:14:"shownumcorrect";s:1:"1";s:23:"showstandardinstruction";s:1:"0";s:7:"answers";a:2:{i:650;O:8:"stdClass":7:{s:2:"id";s:3:"650";s:8:"question";s:3:"207";s:6:"answer";s:44:"<p dir="ltr" style="text-align: left;">1</p>";s:12:"answerformat";s:1:"1";s:8:"fraction";s:9:"1.0000000";s:8:"feedback";s:0:"";s:14:"feedbackformat";s:1:"1";}i:651;O:8:"stdClass":7:{s:2:"id";s:3:"651";s:8:"question";s:3:"207";s:6:"answer";s:44:"<p dir="ltr" style="text-align: left;">2</p>";s:12:"answerformat";s:1:"1";s:8:"fraction";s:9:"0.0000000";s:8:"feedback";s:0:"";s:14:"feedbackformat";s:1:"1";}}}s:5:"hints";a:0:{}s:14:"categoryobject";O:8:"stdClass":9:{s:2:"id";s:2:"83";s:4:"name";s:9:"V4 评价";s:9:"contextid";s:1:"3";s:4:"info";s:125:"<p dir="ltr" style="text-align: left;">我能够根据文中图表和文字的整体效果来评估作者的观点。<br></p>";s:10:"infoformat";s:1:"1";s:5:"stamp";s:33:"moodlenlm.xyz+221124121057+8z2NiB";s:6:"parent";s:1:"3";s:9:"sortorder";s:2:"10";s:8:"idnumber";N;}}